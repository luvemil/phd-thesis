@article{Couchot2016,
abstract = {Let R be a ring (not necessarily commutative). A left R-module is said to be cotorsion if Ext 1 R (G, M) = 0 for any flat R-module G. It is well known that each pure-injective left R-module is cotorsion, but the converse does not hold: for instance, if R is left perfect but not left pure-semisimple then, each left R-module is cotorsion but there exist non-pure-injective left modules. The aim of this paper is to describe the class C of commutative rings R for which each cotorsion R-module is pure-injective. It is easy to see that C contains the class of von Neumann regular rings and the one of pure-semisimple rings. We prove that C is strictly contained in the class of locally pure-semisimple rings. We state that a commutative ring R belongs to C if and only if R verifies one of the following conditions: (1) R is coherent and each pure-essential extension of R-modules is essential; (2) R is coherent and each RD-essential extension of R-modules is essential; (3) any R-module M is pure-injective if and only if Ext 1 R (R/A, M) = 0 for each pure ideal A of R (Baer's criterion).},
archivePrefix = {arXiv},
arxivId = {1506.02924},
author = {Couchot, Fran{\c{c}}ois},
eprint = {1506.02924},
file = {:Users/marco/Dropbox/papers/by{\_}author/couchot/1506.02924v4.pdf:pdf},
journal = {Palestine Journal of Mathematics},
keywords = {()},
number = {1},
pages = {81--89},
title = {{Commutative rings whose cotorsion modules are pure-injective}},
volume = {5},
year = {2016}
}
